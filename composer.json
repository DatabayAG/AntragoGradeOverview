{
  "name": "mbeym/antrago-grade-overview",
  "description": "An ILIAS plugin that adds a grade overview where grades can be imported using csv",
  "type": "project",
  "license": "GPL",
  "authors": [
    {
      "name": "Marvin Beym",
      "email": "mBeym@databay.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "ILIAS\\Plugin\\AntragoGradeOverview\\": "src/"
    },
    "classmap": ["./src"]
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-iconv": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "captainhook/captainhook": "^5.10",
    "friendsofphp/php-cs-fixer": "^3.11"
  },
  "config": {
    "classmap-authoritative": true
  }
}
